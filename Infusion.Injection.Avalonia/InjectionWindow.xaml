<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:injobj="clr-namespace:Infusion.Injection.Avalonia.InjectionObjects;assembly=Infusion.Injection.Avalonia"
        xmlns:injscr="clr-namespace:Infusion.Injection.Avalonia.Scripts;assembly=Infusion.Injection.Avalonia"
        xmlns:injmain="clr-namespace:Infusion.Injection.Avalonia.Main;assembly=Infusion.Injection.Avalonia"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450"
        Width="350" Height="450"
        CanResize="False"
        ShowInTaskbar="True"
        x:Class="Infusion.Injection.Avalonia.InjectionWindow"
        Title="Infusion - Injection">
  <TabControl>
    <TabItem Name="MainTab" Header="Main">
      <injmain:MainControl Name="Main" />
    </TabItem>
    <TabItem Name="ScriptsTab" Header="Script">
      <injscr:ScriptsControl Name="Scripts" />
    </TabItem>
    <TabItem Name="ObjectTab" Header="Objects">
      <injobj:ObjectsControl Name="Objects" />
    </TabItem>
  </TabControl>
</Window>
